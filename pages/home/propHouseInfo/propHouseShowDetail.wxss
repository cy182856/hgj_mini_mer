.house_detail_content{
  margin: 10px;
  background-color: #fff;
  border-radius: 20rpx;
  font-size: 28rpx;
  padding: 20rpx 30rpx;
  line-height: 50rpx;
  color: #3C3C3C;
}
.house_detail_content_addr{
  font-weight: bold;
}
.uploadPic {
  padding-top: 60rpx;
  box-sizing: border-box;
}
 
.uploadPic-ul {
  box-sizing: border-box;
  width: 100%;
  display: flex;
}
 
.uploadPic-ul-wrap {
  /* width: 765rpx; */
  height: 500rpx;
  width: 540rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
 
.uploadPic-li {
  width: 150rpx;
  height: 150rpx;
  position: relative;
  float: left;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
}
 
.uploadPic-li .uploadPic-li-camer {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center 37rpx;
  background-size: 40rpx 35rpx;
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
  top: 0;
  left: 0;
  box-sizing: border-box;
  padding-top: 86rpx;
  text-align: center;
  font-size: 24rpx;
  color: #aaa;
  line-height: 28rpx;
}
 
.uploadPic-li-pic {
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  left: 0;
  top: 0;
}
 
movable-view, movable-view .item-move, movable-view .item-move image {
  width: 150rpx;
  height: 150rpx;
}
 
.uploadPic-li-fm {
  width: 100%;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  background-color: #ff7500;
  font-size: 24rpx;
  color: #fff;
  position: absolute;
  left: 0;
  bottom: 0;
}
 
.uploadPic-li-close {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 23rpx 23rpx;
  width: 23rpx;
  height: 23rpx;
  padding: 10rpx;
  position: absolute;
  right: -22rpx;
  top: -22rpx;
}
 
.uploadPicB {
  width: 678rpx;
  position: fixed;
  bottom: 0;
  left: 50%;
  margin-left: -339rpx;
  padding-bottom: 90rpx;
}
 
.uploadPicB-msg {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 21rpx 27rpx;
  padding-left: 33rpx;
  color: #666;
  font-size: 28rpx;
  line-height: 32rpx;
}
 
.uploadPicB-btn {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  border-radius: 10rpx;
  margin-top: 35rpx;
  background-color: #ff7500;
}
 
.uploadPic-li-msg {
  width: 100%;
  height: 100%;
  position: absolute;
  background-color: #f2f2f2;
  top: 0;
  left: 0;
  box-sizing: border-box;
  padding-top: 55rpx;
  text-align: center;
  font-size: 24rpx;
  color: #aaa;
  line-height: 28rpx;
  word-break: break-all;
  word-wrap: break-word;
  z-index: 1;
}
 
movable-area {
  width: 100%;
  height: 100%;
}

.van-uploader__preview-delete {
  position: absolute;
  top: 0;
  right: 0;
  width: 14px;
  height: 14px;
  background-color: rgba(0,0,0,.7);
  border-radius: 0 0 0 12px;
}

.van-uploader__preview-delete-icon {
  position: absolute;
  top: -2px;
  right: -2px;
  color: #fff;
  font-size: 16px;
  -webkit-transform: scale(.5);
  transform: scale(.5);
}

van-uploader__upload-icon{color:#dcdee0;font-size:24px}
.van-uploader__upload-text{margin-top:8px;color:#969799;font-size:12px}

.van-uploader__file-icon{color:#646566;font-size:20px}
.house_detail_upload{
  background-color: #189AFE;
  padding: 20rpx 0rpx;
  border-radius: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-bottom: 30rpx;
}
.house_detail_upload_btn{
  display: flex;
  justify-content: center;
  color: white;
  font-size: 32rpx;
}

.obj_intru_list{
  text-align: center;
  padding-left: 32rpx;
  padding-right: 38rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  font-size: 30rpx;
}
.obj_intru{
  margin-bottom: 10rpx;
  line-height: 44rpx
}
.obj_title{
  text-align: center;
  color: #323233;
  line-height: 44rpx;
  margin-left: 10rpx;
}
.obj_instru_close_btn{
  font-size: 32rpx;
  padding: 30rpx;
  color: #189AFE;
  text-align: center;
  border-radius: 20rpx;
}
.deadline{
  border-bottom: 3rpx solid #f2f2f2;
}
.house_detail_info_stat{
  display: flex;
  margin-top: 30rpx;
}

.house_detail_info_stat_left{
  width: 50%;
  display: flex;
  align-items: center;
  align-content: center;
}
.house_detail_info_stat_right{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  align-content: center;
  width: 50%;
}
.house_detail_info_stat_upd{
  background-color: #189AFE;
  color: white;
  padding: 5rpx 40rpx;
  border-radius: 20rpx;
}
.iconfont{
  font-size: 36rpx;
  color: #189AFE;
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}
.switch_custom{
  font-size: 36rpx !important;
}